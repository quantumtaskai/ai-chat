{
  "name": "ai-chat-template",
  "version": "1.0.0",
  "description": "Professional AI Chat Template - Modern 2025 UI/UX with embeddable widget for any business website",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:prod": "NODE_ENV=production vite build",
    "preview": "vite preview",
    "type-check": "vue-tsc --noEmit",
    "docker:build": "docker build -t ai-business-receptionist .",
    "docker:run": "docker run -p 8080:80 ai-business-receptionist",
    "docker:compose": "docker-compose up -d",
    "docker:compose:build": "docker-compose up -d --build",
    "docker:logs": "docker-compose logs -f app",
    "clean": "rm -rf dist node_modules package-lock.json",
    "install:clean": "npm run clean && npm install",
    "setup": "node generate-config.js",
    "validate": "node validate-setup.js",
    "template:setup": "./setup.sh"
  },
  "dependencies": {
    "@headlessui/vue": "^1.7.0",
    "@heroicons/vue": "^2.0.0",
    "@tanstack/vue-query": "^5.90.2",
    "@tanstack/vue-virtual": "^3.13.12",
    "axios": "^1.6.0",
    "pinia": "^2.1.0",
    "vue": "^3.4.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@vitejs/plugin-vue": "^5.0.0",
    "autoprefixer": "^10.4.0",
    "postcss": "^8.4.0",
    "tailwindcss": "^3.4.0",
    "terser": "^5.44.0",
    "typescript": "^5.3.0",
    "vite": "^5.0.0",
    "vue-tsc": "^1.8.0"
  }
}
